<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/splash_screen2"
        android:label="@string/app_name"
        android:roundIcon="@drawable/splash_screen2"
        android:supportsRtl="true"
        android:theme="@style/Theme.Challenge5GameSuit"
        tools:targetApi="31">
        <activity
            android:name=".startup.SplashScreenActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".startup.LandingPageActivity"
            android:windowSoftInputMode="adjustPan|adjustResize" />
        <activity android:name=".TypeGameActivity" />
    </application>

</manifest>